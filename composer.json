{
	"name": "tobimori/kirby-dreamform",
	"description": "Kirby DreamForm",
	"type": "kirby-plugin",
	"version": "0.1.0",
	"license": "proprietary",
	"homepage": "https://github.com/tobimori/kirby-dreamform",
	"authors": [
		{
			"name": "Tobias MÃ¶ritz",
			"email": "tobias@moeritz.io"
		}
	],
	"autoload": {
		"psr-4": {
			"tobimori\\DreamForm\\": "classes"
		}
	},
	"require": {
		"php": ">=8.2.0",
		"getkirby/composer-installer": "^1.2.1"
	},
	"scripts": {
		"dist": "composer install --no-dev --optimize-autoloader",
		"fix": "php-cs-fixer fix",
		"post-install-cmd": ["php scripts/postinstall.php"]
	},
	"config": {
		"optimize-autoloader": true,
		"allow-plugins": {
			"getkirby/composer-installer": true
		}
	},
	"extra": {
		"kirby-cms-path": false
	}
}
