{
  "name": "tobimori/kirby-oh-dear",
  "description": "Oh Dear Application Checks for Kirby",
  "type": "kirby-plugin",
  "version": "0.1.0",
  "license": "proprietary",
  "homepage": "https://github.com/tobimori/kirby-oh-dear",
  "authors": [
    {
      "name": "Tobias MÃ¶ritz",
      "email": "tobias@moeritz.io"
    }
  ],
  "autoload": {
    "psr-4": {
      "tobimori\\DreamForm\\": "classes"
    }
  },
  "require": {
    "php": ">=8.1.0",
    "getkirby/composer-installer": "^1.2.1"
  },
  "require-dev": {
    "getkirby/cms": "^4.0-alpha.7",
    "friendsofphp/php-cs-fixer": "^3.24",
    "getkirby/cli": "dev-develop"
  },
  "scripts": {
    "dist": "composer install --no-dev --optimize-autoloader",
    "fix": "php-cs-fixer fix"
  },
  "config": {
    "optimize-autoloader": true,
    "allow-plugins": {
      "getkirby/composer-installer": true
    }
  },
  "extra": {
    "kirby-cms-path": false
  }
}
